<command> ::= <opcode> " " <args> "\n"
<response> ::= (<retval> | "nack") "\n"

Echo:
<opcode> ::= "e"
<args> :: <bytes>
<retval> :: <args>

Write EEPROM:
<opcode> ::= "w"
<args> ::= <addrHi> " " <addrLo> " " <size> " " <bytes>
<retval> ::= "ok"

Read EEPROM:
<opcode> ::= "r"
<args> ::= <addr> " " <size>
<retval> ::= <bytes>{size}

<addrHi> ::= <byte>
<addrLo> :: <byte>
<size> ::= <byte>
<bytes> ::= <byte> <bytetail> | ""
<bytetail> ::= " " <byte> <bytetail> | ""
<byte> ::= "00"--"FF"
